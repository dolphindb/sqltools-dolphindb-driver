{
    "name": "sqltools-dolphindb-driver",
    "displayName": "SQLTools DolphinDB Driver",
    "description": "SQLTools driver for DolphinDB",
    "type": "module",
    "version": "0.1.0",
    "publisher": "dolphindb",
    "engines": {
        "vscode": ">=1.68.0"
    },
    "main": "./out/extension.cjs",
    "scripts": {
        "dev": "node --experimental-transform-types ./dev.ts",
        "build": "node --experimental-transform-types ./build.ts",
        "scan": "i18n-scan --input \"**/*.{ts,tsx}\"",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "fix": "eslint --fix \"**/*.{ts,tsx}\""
    },
    "activationEvents": [
        "*",
        "onLanguage:sql",
        "onCommand:sqltools.*"
    ],
    "extensionDependencies": [
        "mtxr.sqltools"
    ],
    "devDependencies": {
        "@types/koa": "^2.15.0",
        "@types/node": "^22.10.3",
        "@types/react": "^19.0.2",
        "@types/react-dom": "^19.0.2",
        "@types/vscode": "^1.68.0",
        "@types/ws": "^8.5.13",
        "eslint": "^9.17.0",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "@ant-design/icons": "^5.5.2",
        "@ant-design/plots": "1.2.5",
        "@sqltools/base-driver": "^0.1.11",
        "@sqltools/types": "^0.1.7",
        "@vscode/debugadapter": "^1.68.0",
        "@vscode/debugprotocol": "^1.68.0",
        "antd": "^5.22.7",
        "dayjs": "^1.11.13",
        "dolphindb": "^3.0.213",
        "koa": "^2.15.3",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-object-model": "^1.2.21",
        "tslib": "^2.8.1",
        "uuid": "^11.0.3",
        "ws": "^8.18.0",
        "xshell": "^1.1.22"
    }
}
